execute("vcsteps=5", ocbox_)
execute("samp=stim.amp  store_vclamp() glyph()", ocbox_)
ocbox_ = ocbox_.v1
ocbox_.map("I/V Clamp Electrode", 174, 138, 250.2, 349.2)
}
objref ocbox_
//End I/V Clamp Electrode

